<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <h2>Videos</h2>
<ul>
    <% videos.forEach(function(video) { %>
        <li>
          
          <a href="https://www.youtube.com/watch?v=<%= video.youid %>"><%= video.youid %> </a>
        </li>
    <% }); %>
</ul>

    

   


  </body>
</html>
